<script src="https://code.jquery.com/jquery-1.11.2.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/jquery-imagemapster@1.2.10/dist/jquery.imagemapster.min.js"></script>
<script src="/static/js/menuFullpage.min.js"></script>
<script>
        smoothScroll.init();
        $(document).ready(function() {
                $('.menu-point').menuFullpage({
                	menu:"#menu",
					push:".push",

					side:"left",

					menuWidth:"50%",

					speed:"600",

					activeBtn:"menu-open"

                });

                $('.menu-point1').menuFullpage({
                	menu:"#menu1",
					push:".push",

					side:"left",

					menuWidth:"100%",

					speed:"600",

					activeBtn:"menu-open"

                });


                $('#myimage').mapster({
			        fillColor: '4b543d',
			        fillOpacity: 0.3,
			        clickNavigate: true
			    });
        });

        $(document).ready(function(){


        	// get category for menu
        	$.ajax({
			  method: "GET",
			  url: "/admin/getcategory",
			  dataType: "json",
			  success: function(data)
			  {
			  	// console.log(data);
			  	$("#menu ul").html = "";
			  	data.forEach(function(item){
			  		let li_element = "<li><a href='/category/"+ item.category_name +"?page=1'>"+ item.category_name +"</a></li>";
			  		$("#menu ul").append(li_element);
			  	});
			  },
			  error: function(error){
			  	console.log(error);
			  }
			});
        });

</script>

</body>
</html>


















