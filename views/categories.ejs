<!doctype html>

<html lang="en">
<head>
  <meta charset="utf-8">

  <title>High Country Life</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" href="/static/css/style.css"/>

</head>

<body>


<% include adminHead %>

<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h1>Category</h1>
      <a href="/admin/Category/new">Add Category</a>
      <table id="table" class="table" style="table-layout: fixed;">  
              <thead>  
                  <tr>  
                      <th>ID</th>  
                      <th>Name</th> 
                      <th>Image</th> 
                      <th>Action</th>
                  </tr>  
              </thead>  
               <tbody>  
               <% obj.forEach(function (user) { %>
                  <tr>  
                      <td><%= user.id %></td>  
                      <td><%= user.category_name %></td>
                      <td><%= user.cat_img %></td>
                      <td>
                        <a href="/admin/catedit/<%=user.id%>" class="btn btn-primary">edit</a>
                        <a href="/admin/catdelete/<%=user.id%>?_method=DELETE" class="btn btn-danger">delete</a>
                      </td>
                  </tr>                                   
               <% }) %>
               </tbody>
      </table>
    </div>
  </div>
</div>





</body>
</html>





