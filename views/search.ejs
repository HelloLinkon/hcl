<script>
        smoothScroll.init();
        $(document).ready(function() {

        	 	
        		console.log("ready");


                $('.menu-point').menuFullpage({
                	menu:"#menu",
					push:".push",

					side:"right",

					menuWidth:"50%",

					speed:"600",

					activeBtn:"menu-open"

                });

                $('.menu-point1').menuFullpage({
                	menu:"#menu1",
					push:".push",

					side:"right",

					menuWidth:"100%",

					speed:"600",

					activeBtn:"menu-open"

                });


                

			    // img mapster resize
			    var resizeTime = 100;     // total duration of the resize effect, 0 is instant
				var resizeDelay = 100;    // time to wait before checking the window size again
				                          // the shorter the time, the more reactive it will be.
				                          // short or 0 times could cause problems with old browsers.
				                          
				$('#myimage').mapster({
			        fillColor: '4b543d',
			        fillOpacity: 0.3,
			        clickNavigate: true
			    });
				// Resize the map to fit within the boundaries provided

				function resize(maxWidth,maxHeight) {
				     var image =  $('#myimage'),
				        imgWidth = image.width(),
				        imgHeight = image.height(),
				        newWidth=0,
				        newHeight=0;

				    if (imgWidth/maxWidth>imgHeight/maxHeight) {
				        newWidth = maxWidth;
				    } else {
				        newHeight = maxHeight;
				    }
				    image.mapster('resize',newWidth,newHeight,resizeTime);   
				}

				// Track window resizing events, but only actually call the map resize when the
				// window isn't being resized any more

				function onWindowResize() {
				    
				    var curWidth = $(window).width(),
				        curHeight = $(window).height(),
				        checking=false;
				    if (checking) {
				        return;
				            }
				    checking = true;
				    window.setTimeout(function() {
				        var newWidth = $(window).width(),
				           newHeight = $(window).height();
				        if (newWidth === curWidth &&
				            newHeight === curHeight) {
				            resize(newWidth,newHeight); 
				        }
				        checking=false;
				    },resizeDelay );
				}

				$(window).bind('resize',onWindowResize);
			    // end img resize


			    // get category for menu
        	$.ajax({
			  method: "GET",
			  url: "/admin/getcategory",
			  dataType: "json",
			  success: function(data)
			  {
			   

			    // console.log(data);
			    var category_select = document.getElementById("category_select");
			    makeSelectOption(data, category_select, "category_name");

			  	$("#menu ul").html = "";
			  	data.forEach(function(item){
			  		let li_element = "<li><a href='/category/"+ item.category_name +"?page=1'>"+ item.category_name +"</a></li>";
			  		$("#menu ul").append(li_element);
			  	});
			  },
			  error: function(error){
			  	console.log(error);
			  }
			});

			//get city
			$.ajax({
			  method: "GET",
			  url: "/admin/getcity",
			  dataType: "json",
			  success: function(data)
			  {
			   

			    // console.log(data);
			    var city_select = document.getElementById("city_select");
			    makeSelectOption(data, city_select, "city_name");

		
			  },
			  error: function(error){
			  	console.log(error);
			  }
			});
			// get county
			$.ajax({
			  method: "GET",
			  url: "/admin/getcounty",
			  dataType: "json",
			  success: function(data)
			  {
			   

			    // console.log(data);
			    var county_select = document.getElementById("county_select");
			    makeSelectOption(data, county_select, "county");

		
			  },
			  error: function(error){
			  	console.log(error);
			  }
			});

			$("#mobMenu > .row").css("opacity" , "1");




			// scroll reveal
        		ScrollReveal();
        		ScrollReveal().reveal('.text_section', { delay: 200 });
        		ScrollReveal().reveal('.row', { delay: 200 });



        	//search
        	$("#btn_search").click(function(event){
        		
        		console.log("click");
        		var text = $("#search_text").val();
        		console.log(text);
        		$.ajax({
				  method: "GET",
				  url: "/search/"+ text,
				  dataType: "json",
				  success: function(data)
				  {
				   

				    console.log(data);
				    var search_result = $(".search_result");

				    $(".search_result").html("");

				    if(data.length > 0){
				    	data.forEach(function(item){

				    		var list = '<div class="row"><div class="col-sm-12"><div class="business_box" style="margin: 10px;"><div class="business_img" style="background-image: url('+item.image+'); background-size: cover;"></div><div class="business_info"><h1>'+item.title+'</h1><a href="tel:'+item.phone+'"><i class="fas fa-phone"></i> '+item.phone+'</a><a href="https://maps.google.com/?q='+item.address+'" target="_blank"><address><i class="fas fa-map-marker-alt"></i><span style="padding-left: 15px;">'+ item.address +'</span></address></a><p style="width: 90%; margin: 10px 0;">one</p><a href="'+item.website+'" class="btn btn-warning" style="width: 30%; margin-bottom: 20px;">Website</a></div></div></div></div>';
				    		

				    		search_result.append(list);
				    	});
				    }
				    else{
				       search_result.append("<h1>No Result</h1>");
				    }
				    
				  },
				  error: function(error){
				  	console.log(error);
				  }
				});
        	});

        	// Filter
        	$("#btn_filter").click(function(){

        		var text = $("#search_text").val();
        		var city = $("#city_select").val();
        		var county = $("#county_select").val();
        		var category = $("#category_select").val();
        		console.log(text);
        		$.ajax({
				  method: "GET",
				  url: "/search/"+ text +"/" + city + "/" + county +"/" + category ,
				  dataType: "json",
				  success: function(data)
				  {
				   

				    console.log(data);
				    var search_result = $(".search_result");


				    $(".search_result").html("");
				    if(data.length > 0){
				    	data.forEach(function(item){

				    		var list = '<div class="row"><div class="col-sm-12"><div class="business_box" style="margin: 10px;"><div class="business_img" style="background-image: url('+item.image+'); background-size: cover;"></div><div class="business_info"><h1>'+item.title+'</h1><a href="tel:'+item.phone+'"><i class="fas fa-phone"></i> '+item.phone+'</a><a href="https://maps.google.com/?q='+item.address+'" target="_blank"><address><i class="fas fa-map-marker-alt"></i><span style="padding-left: 15px;">'+ item.address +'</span></address></a><p style="width: 90%; margin: 10px 0;">one</p><a href="'+item.website+'" class="btn btn-warning" style="width: 30%; margin-bottom: 20px;">Website</a></div></div></div></div>';
				    		

				    		search_result.append(list);
				    	});
				    }
				    else{
				       search_result.append("<h1>No Result</h1>");
				    }
				    
				  },
				  error: function(error){
				  	console.log(error);
				  }
				});


        	});


        	function makeSelectOption(data, parent, child){
        		// console.log(data);
        		data.forEach(function(item){

        			
        			var option = document.createElement("option");
        			option.value = item.id;
        			option.text = item[child];
        			parent.appendChild(option);
        		});
        	}


        });

       

</script>
